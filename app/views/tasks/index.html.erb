<h1>Listing tasks</h1>

<table>
  <thead>
    <tr>
      <th>Kategoria</th>
      <th>Lista</th>
      <th>Zadanie</th>
      <th>Url</th>
      <th>Status</th>
      <th>Poziom trudno≈õci</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td><%= task.list.category.name %></td>
        <td><%= task.list.name %></td>
        <td><%= task.name %></td>
        <td><%= task.url %></td>
        <td><%= task.status %></td>
        <td><%= task.level %></td>
        <td><%= link_to 'Show', task %></td>
      </tr>
    <% end %>
  </tbody>
</table>

